<center><h1>Nworld User.  <h1></center>  

<table width="1000">
	<thead>
		<tr>
			<th>Username </th>
			<th>Email </th>
			<th>Type </th>
			<th>First Name </th>
			<th>Last Name </th>
			<th>Gender </th>
			<th>Birthday</th>
			<th>Sign In Count </th>
			<th>Last Session </th>
			<th>Last Sign in IP </th>
		</tr>
	</thead>

	<tbody>
		<% @user.each do |user| %>
			<tr>
				<td><%=user.username%></td>
				<td><%=user.email%></td>
				<td><%=user.usertype%></td>
				<td><%=user.first%></td>
				<td><%=user.last%></td>
				<td><%=user.sex%></td>
				<td><%=user.birthday%></td>
				<td><%=user.sign_in_count%></td>
				<td><%=user.last_sign_in_at%></td>
				<td><%=user.last_sign_in_ip%></td>
			</tr>
		<% end %>
	</tbody>
</table>

<a href="/products"> Back to Main </a><br>